{
   "version": 2,
   "name": "multistore-ecommerce-api",
   "builds": [
     {
       "src": "src/index.js",
       "use": "@vercel/node",
       "config": {
         "includeFiles": ["*.env"] 
       }
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "src/app.js"
     }
   ]
 }
 
 
 