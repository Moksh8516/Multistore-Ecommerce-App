{
  "version":1,
  "name": "Multistore-website",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    { "src": "/api/v1/user", "dest": "./src/app.js" },
    { "src": "/api/product", "dest": "./src/app.js" },
    { "src": "/api/brands", "dest": "./src/app.js" },
    { "src": "/api/categories", "dest": "./src/app.js" },
    { "src": "/api/address", "dest": "./src/app.js" },
    { "src": "/api/review", "dest": "./src/app.js" },
    { "src": "/api/cart", "dest": "./src/app.js" },
    { "src": "/api/order", "dest": "./src/app.js" },
    { "src": "/api/payment", "dest": "./src/app.js" },
    { "src": "/(.*)", "dest": "./src/app.js" }
  ]
}
