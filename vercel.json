{
   "version": 2,
   "name": "multistore-ecommerce-api",
   "builds": [
     {
       "src": "./src/index.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "./src/app.js"
     }
   ],

      "rewrites": [{ "source": "/(.*)", "destination": "./src/app.js" }]
    
 }
 